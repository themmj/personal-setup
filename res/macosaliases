export CC=/usr/local/bin/gcc-10
export CXX=/usr/local/bin/g++-10
export CPP=/usr/local/bin/cpp-10
export LD=/usr/local/bin/gcc-10
alias gcc=$CC
alias c++=/usr/local/bin/c++-10
alias g++=$CXX
alias cpp=$CPP
alias ld=$CC
alias cc=$CC
alias gcov=/usr/local/bin/gcov-10
alias gcov-dump=/usr/local/bin/gcov-dump-10
alias gcov-tool=/usr/local/bin/gcov-tool-10
